{
  "name": "json-parser",
  "version": "0.1.0",
  "private": false,
  "description": "json parser",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "source": "./src/index.ts",
  "scripts": {
    "build": "rimraf ./dist && microbundle",
    "dev": "microbundle watch",
    "test": "jest",
    "changeset": "changeset",
    "ci:version": "changeset version",
    "ci:publish": "pnpm run build && pnpm run test && pnpm publish --no-git-checks"
  },
  "keywords": [
    "json",
    "parser"
  ],
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "devDependencies": {
    "@types/jest": "^28.1.3",
    "@types/node": "^18.0.0",
    "jest": "^28.1.1",
    "microbundle": "^0.15.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.1",
    "prettier": "^2.5.1",
    "lint-staged": "^12.3.7",
    "husky": "8",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "@changesets/changelog-github": "^0.4.4",
    "@changesets/cli": "^2.22.0",
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1"
  },
  "author": "zyrong",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "npm run prettier",
      "npm run eslint:fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "bugs": {
    "url": "https://github.com/zyrong/json-parser/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zyrong/json-parser"
  },
  "homepage": "https://github.com/zyrong/json-parser",
  "license": "MIT"
}
